{
  "name": "decoration-vuex",
  "version": "2.1.0",
  "description": "Create type-safe class-based Vuex modules in TypeScript",
  "private": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "lint": "eslint src tests rolete.config.js",
    "fix": "eslint --fix src tests rolete.config.js",
    "dev": "rolete -c dev",
    "prod": "rolete -c prod",
    "coverage": "nyc ava",
    "test": "ava",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run prod",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/6XGate/decoration-vuex.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "typescript"
  ],
  "author": "Matthew Holder (https://www.sleepingcats.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/6XGate/decoration-vuex/issues"
  },
  "homepage": "https://github.com/6XGate/decoration-vuex#readme",
  "engines": {
    "node": ">=12.9.0"
  },
  "files": [
    "dist/**/*"
  ],
  "rolete": {
    "name": "DecorationVuex",
    "input": "src/index.ts",
    "targets": {
      "cjs": "#main",
      "esm": "#module",
      "iife": "dist/index.iife.js"
    },
    "globals": {
      "lodash": "_",
      "vue": "Vue",
      "vue-class-component": "VueClassComponent",
      "vuex": "Vuex"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "esm"
    ],
    "nodeArguments": [
      "--enable-source-maps"
    ]
  },
  "peerDependencies": {
    "vuex": "^3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@rolete/rolete": "^1.1.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "ava": "^3.15.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "esm": "^3.2.25",
    "husky": "^6.0.0",
    "nyc": "^15.1.0",
    "pinst": "^2.1.6",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "type-fest": "^0.21.3",
    "typescript": "^4.2.3",
    "vue": "^2.6.12",
    "vuex": "^3.6.2"
  }
}
